{
  "version": 2,
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/staff/pages/login.html"
    },
    {
      "src": "/staff/pages/(.*)",
      "dest": "/staff/pages/$1"
    },
    {
      "src": "/staff/(.*\\.(?:js|css|png|jpg|svg|ico))",
      "dest": "/staff/$1"
    },
    {
      "src": "/student/pages/(.*)",
      "dest": "/student/pages/$1"
    },
    {
      "src": "/student/(.*\\.(?:js|css|png|jpg|svg|ico))",
      "dest": "/student/$1"
    },
    {
      "src": "/shared/(.*)",
      "dest": "/shared/$1"
    }
  ]
}