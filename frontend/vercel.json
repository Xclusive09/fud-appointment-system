{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/staff/pages/login.html"
    },
    {
      "src": "/staff/(.*)",
      "dest": "/staff/pages/$1"
    },
    {
      "src": "/students/(.*)",
      "dest": "/students/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/staff/pages/$1"
    }
  ]
}